<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorPrimaryDark">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?actionBarSize"
            android:background="@color/colorPrimaryDark"
            android:theme="@style/ToolbarColoredBackArrow"
            app:subtitleTextColor="@color/colorWhite"
            app:titleTextColor="@color/colorWhite"></androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/toolbar"
            android:layout_marginTop="10dp"
            android:background="@drawable/curve_main_bg">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.core.widget.NestedScrollView
                    android:layout_marginTop="0dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_above="@+id/cardBottom"
                    android:fillViewport="true">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imgBanner"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:scaleType="fitXY">

                        </ImageView>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="16dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtPageHeading"
                                android:layout_marginTop="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorPrimaryDark"
                                android:text="@string/app_name"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:fontFamily="@font/nunit_sans_bold"
                                android:textSize="20sp"></TextView>

                            <TextView
                                android:id="@+id/txtQuotes"
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/textDark"
                                android:text="@string/app_name"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:fontFamily="@font/nunito_sans_regular"
                                android:textSize="18sp"></TextView>

                            <TextView
                                android:id="@+id/txtDesc"
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/textDark"
                                android:text="@string/app_name"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:fontFamily="@font/nunito_sans_regular"
                                android:textSize="16sp"></TextView>


                            <LinearLayout
                                android:layout_marginTop="20dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/txtOurHeritageTitle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:text="@string/app_name"
                                    android:fontFamily="@font/nunito_sans_semibold"
                                    android:textSize="18sp"></TextView>

                                <LinearLayout
                                    android:layout_marginTop="10dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <androidx.cardview.widget.CardView
                                        android:layout_width="120dp"
                                        android:layout_height="120dp"
                                        app:cardCornerRadius="5dp"
                                        app:cardElevation="0dp">

                                        <ImageView
                                            android:id="@+id/imgOurHeritage"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:adjustViewBounds="true"
                                            android:scaleType="fitXY"
                                            android:src="@drawable/ic_no_car"></ImageView>

                                    </androidx.cardview.widget.CardView>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginRight="10dp"
                                        android:layout_marginLeft="10dp"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/txtOurHeritageDesc"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/textDark"
                                            android:text="@string/app_name"
                                            android:fontFamily="@font/nunito_sans_regular"
                                            android:textSize="16sp"></TextView>

                                        <TextView
                                            android:id="@+id/txtOurHeritageMore"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/lightBlue"
                                            android:text="@string/moreView1"
                                            android:layout_marginTop="5dp"
                                            android:fontFamily="@font/nunito_sans_regular"
                                            android:textSize="14sp"></TextView>

                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:alpha="0.5"
                            android:background="@color/textDark"></LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="16dp"
                            android:orientation="vertical">

                           <LinearLayout
                               android:layout_width="match_parent"
                               android:layout_height="wrap_content"
                               android:orientation="horizontal">

                               <ImageView
                                   android:id="@+id/imgOurMission"
                                   android:layout_width="50dp"
                                   android:layout_height="50dp"
                                   android:src="@drawable/ic_no_car"></ImageView>
                               <TextView
                                   android:id="@+id/txtOurMissionTitle"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:textColor="@color/colorPrimaryDark"
                                   android:text="@string/app_name"
                                   android:paddingLeft="15dp"
                                   android:paddingRight="15dp"
                                   android:layout_gravity="center"
                                   android:fontFamily="@font/nunito_sans_semibold"
                                   android:textSize="18sp"></TextView>

                           </LinearLayout>

                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/txtOurMissionDesc"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/textDark"
                                        android:text="@string/app_name"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="14sp"></TextView>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:alpha="0.5"
                            android:background="@color/textDark"></LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="16dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/imgOurVission"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:src="@drawable/ic_no_car"></ImageView>
                                <TextView
                                    android:id="@+id/txtOurVissionTitle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:text="@string/app_name"
                                    android:paddingLeft="15dp"
                                    android:paddingRight="15dp"
                                    android:layout_gravity="center"
                                    android:fontFamily="@font/nunito_sans_semibold"
                                    android:textSize="18sp"></TextView>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/txtOurVissionDesc"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/textDark"
                                        android:text="@string/app_name"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="14sp"></TextView>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:alpha="0.5"
                            android:background="@color/textDark"></LinearLayout>



                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="16dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/txtOurValueTitle"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:text="@string/ourValue"
                                    android:layout_gravity="center"
                                    android:fontFamily="@font/nunito_sans_semibold"
                                    android:textSize="18sp"></TextView>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <ImageView
                                            android:id="@+id/imgOurValue1"
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginRight="10dp"
                                            android:src="@drawable/ic_no_car"></ImageView>

                                        <TextView
                                            android:id="@+id/txtOurValueTitle1"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:text="@string/app_name"
                                            android:layout_gravity="center"
                                            android:fontFamily="@font/nunito_sans_semibold"
                                            android:textSize="18sp"></TextView>

                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/txtOurValueDesc1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/textDark"
                                        android:text="@string/app_name"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="14sp"></TextView>

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <ImageView
                                            android:id="@+id/imgOurValue2"
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginRight="10dp"
                                            android:src="@drawable/ic_no_car"></ImageView>

                                        <TextView
                                            android:id="@+id/txtOurValueTitle2"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:text="@string/app_name"
                                            android:layout_gravity="center"
                                            android:fontFamily="@font/nunito_sans_semibold"
                                            android:textSize="18sp"></TextView>

                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/txtOurValueDesc2"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/textDark"
                                        android:text="@string/app_name"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="14sp"></TextView>

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <ImageView
                                            android:id="@+id/imgOurValue3"
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginRight="10dp"
                                            android:src="@drawable/ic_no_car"></ImageView>

                                        <TextView
                                            android:id="@+id/txtOurValueTitle3"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:text="@string/app_name"
                                            android:layout_gravity="center"
                                            android:fontFamily="@font/nunito_sans_semibold"
                                            android:textSize="18sp"></TextView>

                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/txtOurValueDesc3"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/textDark"
                                        android:text="@string/app_name"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="14sp"></TextView>

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <ImageView
                                            android:id="@+id/imgOurValue4"
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginRight="10dp"
                                            android:src="@drawable/ic_no_car"></ImageView>

                                        <TextView
                                            android:id="@+id/txtOurValueTitle4"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:text="@string/app_name"
                                            android:layout_gravity="center"
                                            android:fontFamily="@font/nunito_sans_semibold"
                                            android:textSize="18sp"></TextView>

                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/txtOurValueDesc4"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/textDark"
                                        android:text="@string/app_name"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="14sp"></TextView>

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <ImageView
                                            android:id="@+id/imgOurValue5"
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:layout_marginLeft="10dp"
                                            android:layout_marginRight="10dp"
                                            android:src="@drawable/ic_no_car"></ImageView>

                                        <TextView
                                            android:id="@+id/txtOurValueTitle5"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimaryDark"
                                            android:text="@string/app_name"
                                            android:layout_gravity="center"
                                            android:fontFamily="@font/nunito_sans_semibold"
                                            android:textSize="18sp"></TextView>

                                    </LinearLayout>

                                    <TextView
                                        android:id="@+id/txtOurValueDesc5"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/textDark"
                                        android:text="@string/app_name"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="14sp"></TextView>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:alpha="0.5"
                            android:background="@color/textDark"></LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="16dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtOurFleetTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorPrimaryDark"
                                android:text="@string/app_name"
                                android:fontFamily="@font/nunito_sans_semibold"
                                android:textSize="18sp"></TextView>

                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <androidx.cardview.widget.CardView
                                    android:layout_width="120dp"
                                    android:layout_height="120dp"
                                    app:cardCornerRadius="5dp"
                                    app:cardElevation="0dp">

                                    <ImageView
                                        android:id="@+id/imgOurFleet"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:adjustViewBounds="true"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/ic_no_car"></ImageView>

                                </androidx.cardview.widget.CardView>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="10dp"
                                    android:layout_marginLeft="10dp"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/txtOurFleetDesc"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/textDark"
                                        android:text="@string/app_name"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="16sp"></TextView>

                                    <TextView
                                        android:id="@+id/txtOurFleetMore"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/lightBlue"
                                        android:text="@string/moreView1"
                                        android:layout_marginTop="5dp"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="14sp"></TextView>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:alpha="0.5"
                            android:background="@color/textDark"></LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="16dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtMileTitle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorPrimaryDark"
                                android:text="@string/app_name"
                                android:fontFamily="@font/nunito_sans_semibold"
                                android:textSize="18sp"></TextView>

                            <LinearLayout
                                android:layout_marginTop="10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <androidx.cardview.widget.CardView
                                    android:layout_width="120dp"
                                    android:layout_height="120dp"
                                    app:cardCornerRadius="5dp"
                                    app:cardElevation="0dp">

                                    <ImageView
                                        android:id="@+id/imgMile"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:adjustViewBounds="true"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/ic_no_car"></ImageView>

                                </androidx.cardview.widget.CardView>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="10dp"
                                    android:layout_marginLeft="10dp"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/txtMileDesc"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/textDark"
                                        android:text="@string/app_name"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="16sp"></TextView>

                                    <TextView
                                        android:id="@+id/txtMileMore"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/lightBlue"
                                        android:text="@string/moreView1"
                                        android:layout_marginTop="5dp"
                                        android:fontFamily="@font/nunito_sans_regular"
                                        android:textSize="14sp"></TextView>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>


                    </LinearLayout>


                </androidx.core.widget.NestedScrollView>

                <TextView
                    android:id="@+id/txtError"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/noDataFound"
                    android:textSize="22sp"
                    android:visibility="gone"
                    android:layout_centerInParent="true"
                    android:textColor="@color/colorPrimaryDark"
                    android:fontFamily="@font/nunito_sans_semibold">

                </TextView>

            </RelativeLayout>


        </LinearLayout>

    </RelativeLayout>

</layout>